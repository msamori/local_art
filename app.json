{
  "expo": {
    "name": "Local Art",
    "slug": "local_art",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher_round.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/blueno_splash-modified.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_round.png",
        "backgroundColor": "#000000"
      },
      "config": {
        "googleMaps": {
          "apiKey": "API KEY HERE"
        }
      },
      "package": "com.msamori.local_art",
      "permissions": ["READ_MEDIA_IMAGES", "ACCESS_MEDIA_LOCATION", "READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/ic_launcher.png"
    }
  }
}
